CREATE TABLE USERS(
	ID VARCHAR2(16),
	PWD VARCHAR2(16) NOT NULL,
	NICKNAME VARCHAR2(16) NOT NULL,
	EMAIL VARCHAR2(32) NOT NULL,
	JOINDATE VARCHAR2(16) NOT NULL,
	PRIMARY KEY (ID)
);

CREATE TABLE FILES(
	NAME VARCHAR2(16),
	VERSION VARCHAR2(16),
	USER_ID VARCHAR2(16) NOT NULL,
	CONTENT VARCHAR2(256) NOT NULL,
	SINCE VARCHAR2(16) NOT NULL,
	REPOSITORY_PATH VARCHAR2(64) NOT NULL,
	DOWNLOAD_COUNT NUMBER(8) DEFAULT 0,
	PRIMARY KEY(NAME, VERSION),
	FOREIGN KEY(USER_ID) REFERENCES USERS(ID)
);